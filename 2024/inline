  {
   "cell_type": "code",
   "execution_count": 40,
   "id": "aa962d7a",
   "metadata": {},
   "outputs": [],
   "source": [
    "init_config = []\n",
    "width, height = ((101, 103) if len(config) > 50 else (11,7))\n",
    "grid = np.zeros((width, height), dtype=int)\n",
    "\n",
    "t=0\n",
    "for robot in config:\n",
    "    init_config.append((robot[0], robot[1]))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 41,
   "id": "44d53bf3",
   "metadata": {},
   "outputs": [],
   "source": [
    "grid = np.zeros((width, height), dtype=int)\n",
    "t+=1\n",
    "for robot in config:\n",
    "    final_config.append(( (robot[0] + t * robot[2])%width, (robot[1] + t * robot[3])%height))\n",
    "    grid[robot[0], robot[1]] += 1\n",
