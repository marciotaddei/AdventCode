  },
  {
   "cell_type": "code",
   "execution_count": 357,
   "id": "978cf2ba",
   "metadata": {},
   "outputs": [],
   "source": [
    "import matplotlib.animation as animation\n",
    "\n",
    "fig, ax = plt.subplots()\n",
    "theax = ax.matshow(grid.T, vmax=1)\n",
    "\n",
    "def update_robots(t, init_place):\n",
    "    width, height = (101, 103) \n",
    "    grid = np.zeros((width, height), dtype=int)\n",
    "    for i in range(len(placemt)):\n",
    "        placemt[i] = ( (placemt[i][0] + t*config[i][2])%width, (placemt[i][1] + t*config[i][3])%height)\n",
    "        grid[placemt[i]] += 1\n",
    "    return ax.matshow(grid.T, vmax=1)\n",
    "\n",
    "\n",
    "ani = animation.FuncAnimation(fig, func=lambda t: update_robots(t, init_place), frames=1000, interval=1, repeat=False)\n",
    "plt.show()"
   ]
