    "    ith_hist = []\n",
    "    for t in range(100000):\n",
    "        placemt[i] = ( (placemt[i][0] + config[i][2])%width, (placemt[i][1] + config[i][3])%height)\n",
    "        if placemt[i] in ith_hist:\n",
    "            cycles.append(t)\n",
    "            break\n",
    "        ith_hist.append(placemt[i])\n",
    "cycles"
   ]
