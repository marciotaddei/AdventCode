    "config = []\n",
    "for line in data:\n",
    "    p, v = line.split()\n",
    "    p, v = p.strip(\"p=\").split(\",\"), v.strip(\"v=\").split(\",\")\n",
    "    config.append((int(p[0]), int(p[1]), int(v[0]), int(v[1])))"
