  {
   "cell_type": "code",
   "execution_count": 387,
   "id": "317f2d67",
   "metadata": {},
   "outputs": [],
   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "from matplotlib.animation import FuncAnimation\n",
    "\n",
    "\n",
    "def create_video(n):\n",
    "    global X\n",
    "    X = np.random.binomial(1, 0.3, size = (n, n))\n",
    "\n",
    "    fig = plt.figure()\n",
    "    im = plt.imshow(X, cmap = plt.cm.gray)\n",
    "\n",
    "    def animate(t):\n",
    "        global X\n",
    "        X = np.roll(X, +1, axis = 0)\n",
    "        im.set_array(X)\n",
    "        return im, \n",
    "\n",
    "    anim = FuncAnimation(\n",
    "        fig,\n",
    "        animate,\n",
    "        frames = 100,\n",
    "        interval = 1000/30,\n",
    "        blit = True\n",
    "    )\n",
    "\n",
    "    plt.show()\n",
    "\n",
    "    return anim\n",
    "\n",
    "anim = create_video(10)"
   ]
  },
