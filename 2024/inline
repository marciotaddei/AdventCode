    "init_place = []\n",
    "width, height = ((101, 103) if len(config) > 50 else (11,7))\n",
    "grid = np.zeros((width, height), dtype=int)\n",
    "\n",
    "t=0\n",
    "for robot in config:\n",
    "    init_place.append((robot[0], robot[1]))\n",
    "placemt = init_place.copy()"
   ]
